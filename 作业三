#include <iostream>
#include<vector>
using namespace std;

vector<int>an;
int main() {
    int n;
    cin>>n;

    an.push_back(1);
    an.push_back(2);
    for(int i=0;i<n;i++)
    {
        int k;
        cin>>k;
        if(an.size()>k-1)
        {
            cout<<an[k-1]<<endl;
        }
        else
        {
            for(int i=an.size();i<k;i++)
            {
                int x=2*an[i-1]+an[i-2];
                x=x%32767;
                an.push_back(x);
            }
            cout<<an[k-1]<<endl;
        }
    }

}
// 64 位输出请用 printf("%lld")
