//统计字符
#include<iostream>
#include<string>
using namespace std;
int main()
{
    string str;
    while(getline(cin,str))
    {
        int len=str.length();
        int ZiMu=0,KongGe=0,ShuZi=0,QiTa=0;
        for(int i=0;i<len;i++)
        {
            if(str[i]>='a' && str[i]<='z' )
            ZiMu++;
            else if(str[i]>='A' && str[i]<='Z')
            ZiMu++;
            else if(str[i] == ' ')
            KongGe++;
            else if(str[i]>='0' && str[i]<='9')
            ShuZi++;
            else
             QiTa++;

        }
        cout<<ZiMu<<endl<<KongGe<<endl<<ShuZi<<endl<<QiTa<<endl;
    }
    return 0;
}


